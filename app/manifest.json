{
  "name": "Mooltipass App",
  "description": "Mooltipass password manager client",
  "version": "0.6.5",
  "app": {
    "background": {
      "scripts": [
        "/vendor/mooltipass/helpers.js",
        "/vendor/mooltipass/device.js",
        "/vendor/mooltipass/clients.js",
        "/vendor/mooltipass/interface.js",
        "/vendor/mooltipass/messages.js",
        "/vendor/mooltipass/memmgmt.js",
        "/vendor/mooltipass/memmgmt_hid.js",
        "/vendor/mooltipass/datamemmgmt.js",
        "/vendor/mooltipass/filehandler.js",
        "/vendor/mooltipass/prefstorage.js",
        "/vendor/mooltipass/util.js",
        "/js/init.js"
      ]
    }
  },
  "permissions": [
    "hid",
    {
      "usbDevices": [
        {
          "vendorId": 5840,
          "productId": 2464
        }
      ]
    },
    {
      "fileSystem": [
        "write"
      ]
    },
    "syncFileSystem",
    "storage"
  ],
  "icons": {
    "19": "images/icons/AppIcon_19.png",
    "32": "images/icons/AppIcon_32.png",
    "128": "images/icons/AppIcon_128.png"
  }
}